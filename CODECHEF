#include<iostream>
using namespace std;
int main()
{
	int t;cin>>t;
	while(t--)
	{
		int a[30],fl=1;
		for(int i=0;i<30;i++)
		{
			cin>>a[i];
		}
		int count=0;
		for(int i=0;i<30;i++)
		{
			if(a[i]==1)
			count++;
			else
			count=0;
			if(count==6)
			fl=0;
			}
		if(fl)
		cout<<"#allcodersarefun"<<endl;
		else
		cout<<"#coderlifematters"<<endl;
	}
}
